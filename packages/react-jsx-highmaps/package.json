{
  "name": "react-jsx-highmaps",
  "version": "2.0.1",
  "description": "Highmaps built using React components",
  "main": "dist/react-jsx-highmaps.min.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=development ./node_modules/.bin/webpack",
    "build:prod": "npm run build:umd && npm run build:es",
    "build:umd": "cross-env NODE_ENV=production ./node_modules/.bin/webpack",
    "build:es": "BABEL_ENV=es babel src --out-dir dist/es",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.js\" \"test/**/*.js\" README.md",
    "lint": "eslint src",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "author": "Will Hawker",
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/whawker/react-jsx-highcharts.git"
  },
  "bugs": "https://github.com/whawker/react-jsx-highcharts/issues",
  "homepage": "https://github.com/whawker/react-jsx-highcharts",
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "highcharts",
    "highmaps",
    "chart",
    "charts",
    "graphs",
    "visualization",
    "data",
    "maps"
  ],
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "lodash-es": "^4.17.13",
    "react-jsx-highcharts": "4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^6.0.3",
    "enzyme": "~3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-react-perf": "^3.2.3",
    "highcharts": "^8.0.0",
    "jest": "^25.1.0",
    "jest-enzyme": "^7.1.2",
    "lodash-webpack-plugin": "^0.11.5",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "rimraf": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "peerDependencies": {
    "highcharts": "^8.0.0",
    "prop-types": "^15.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "browserslist": [
    "ie >= 11"
  ]
}
