{
  "name": "react-jsx-highmaps",
  "version": "1.2.0",
  "description": "Highmaps built using React components",
  "main": "dist/react-jsx-highmaps.min.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=development ./node_modules/.bin/webpack",
    "build:prod": "npm run build:umd && npm run build:es",
    "build:umd": "cross-env NODE_ENV=production ./node_modules/.bin/webpack",
    "build:es": "BABEL_ENV=es babel src --out-dir dist/es",
    "clean": "rimraf dist",
    "lint": "./node_modules/.bin/eslint src",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "author": "Will Hawker",
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/whawker/react-jsx-highcharts.git"
  },
  "bugs": "https://github.com/whawker/react-jsx-highcharts/issues",
  "homepage": "https://github.com/whawker/react-jsx-highcharts",
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "highcharts",
    "highmaps",
    "chart",
    "charts",
    "graphs",
    "visualization",
    "data",
    "maps"
  ],
  "dependencies": {
    "@babel/runtime": "^7.4.2",
    "lodash-es": "^4.17.5",
    "react-jsx-highcharts": "~3.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "cross-env": "^5.0.1",
    "enzyme": "~3.9.0",
    "enzyme-adapter-react-16": "~1.11.2",
    "eslint": "^5.15.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "highcharts": "^7.1.2",
    "jest": "^24.5.0",
    "jest-enzyme": "^7.0.2",
    "lodash-webpack-plugin": "^0.11.5",
    "prop-types": "^15.7.2",
    "react": "~16.2.0",
    "react-dom": "~16.2.0",
    "react-test-renderer": "^16.8.5",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "peerDependencies": {
    "highcharts": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "prop-types": "^15.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
