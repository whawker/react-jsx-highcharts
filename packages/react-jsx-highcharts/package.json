    "react": "~16.2.0",
    "react-dom": "~16.2.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.1",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.2",
