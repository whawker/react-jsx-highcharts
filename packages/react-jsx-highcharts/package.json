{
  "name": "react-jsx-highcharts",
  "version": "3.0.1",
  "description": "Highcharts charts built using React components",
  "main": "dist/react-jsx-highcharts.min.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=development ./node_modules/.bin/webpack",
    "build:prod": "npm run build:umd && npm run build:es && npm run build:copyfiles",
    "build:umd": "cross-env NODE_ENV=production ./node_modules/.bin/webpack",
    "build:es": "BABEL_ENV=es babel src --out-dir dist/es",
    "build:copyfiles": "cp ../../README.md . && cp src/index.js.flow dist/react-jsx-highcharts.min.js.flow",
    "lint": "./node_modules/.bin/eslint src",
    "test": "npm run test:mocha && npm run test:flow",
    "test:mocha": "cross-env NODE_ENV=test ./node_modules/.bin/mocha ./test -name '*.spec.js' --recursive --require babel-core/register --require test/test-helper.js",
    "test:flow": "flow check"
  },
  "author": "Will Hawker",
  "contributors": [
    {
      "name": "Alex Mayants",
      "url": "https://github.com/AlexMayants"
    },
    {
      "name": "mrawdon",
      "url": "https://github.com/mrawdon"
    },
    {
      "name": "Ercan Aky√ºrek",
      "url": "https://github.com/geforcefan"
    },
    {
      "name": "anajavi",
      "url": "https://github.com/anajavi"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/whawker/react-jsx-highcharts.git"
  },
  "bugs": "https://github.com/whawker/react-jsx-highcharts/issues",
  "homepage": "https://github.com/whawker/react-jsx-highcharts",
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "highcharts",
    "highstock",
    "chart",
    "charts",
    "graphs",
    "visualization",
    "data"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^1.0.0-beta.0",
    "cross-env": "^5.0.1",
    "enzyme": "~3.3.0",
    "enzyme-adapter-react-16": "~1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.73.0",
    "highcharts": "^6.0.0",
    "html-webpack-include-assets-plugin": "1.0.4",
    "html-webpack-plugin": "^3.2.0",
    "immutable": "^3.8.1",
    "jsdom": "^11.0.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mocha": "^5.1.1",
    "prop-types": "^15.0.0",
    "react": "~16.2.0",
    "react-dom": "~16.2.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.0",
    "sinon-chai": "^2.10.0",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "create-react-context": "^0.2.2",
    "immutable-is": "^3.7.6",
    "is-immutable": "^1.0.1",
    "lodash": "^4.17.5",
    "lodash-es": "^4.17.5",
    "uuid": "^3.2.1"
  },
  "peerDependencies": {
    "highcharts": "^5.0.0 || ^6.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "prop-types": "^15.0.0"
  }
}
